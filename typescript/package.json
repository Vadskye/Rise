  "scripts": {
    "clean": "rm src/**/*.js",
    "coverage": "tsc --incremental && NODE_OPTIONS='--import tsx' tap --exclude=**/*.js --coverage-report=text",
    "script": "tsc --incremental && node --enable-source-maps -r tsconfig-paths/register",
    "maneuver_ranks": "tsc --incremental && NODE_OPTIONS='--import tsx' node src/scripts/maneuver_rank_breakdown.js",
    "maneuver_roles": "tsc --incremental && NODE_OPTIONS='--import tsx' node src/scripts/maneuver_role_breakdown.js",
    "spell_ranks": "tsc --incremental && NODE_OPTIONS='--import tsx' node src/scripts/spell_rank_breakdown.js",
    "spell_roles": "tsc --incremental && NODE_OPTIONS='--import tsx' node src/scripts/spell_role_breakdown.js",
    "tap": "tsc --incremental && NODE_OPTIONS='--import tsx' tap --exclude=**/*.js",
    "test": "tsc --incremental && NODE_OPTIONS='--import tsx' tap --exclude=**/*.js"
  }
}
